{
  "name": "waitpid2",
  "version": "2.0.5",
  "description": "Complete native binding to the waitpid() syscall",
  "main": "lib/waitpid.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mhisaya/node-waitpid2.git"
  },
  "keywords": [
    "waitpid",
    "child process"
  ],
  "author": "joshiggins",
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.8"
  },
  "devDependencies": {
    "node-pre-gyp-github": "^1.4.3"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "scripts": {
    "prepublish": "npm ls",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "binary": {
    "module_name": "waitpid",
    "module_path": "./lib/binding/",
    "host": "https://github.com/mhisaya/node-waitpid2/releases/download/",
    "remote_path": "{version}"  
  }
}
